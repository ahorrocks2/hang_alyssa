<div id="homescreen">
  <h1>HANG YOURSELF</h1>

  <%= form_for @game, url: {action: "create"}, html: { multipart: true } do |f| %>
    <%= f.file_field :avatar %>
    <%= f.submit "BOOKSMART", name: 'book', class: 'btn btn-info'%>
    <%= f.submit "STREETSMART", name: 'street', class: 'btn btn-danger'%>
  <% end %>
</div>
